	script(src="/assets/js/bootstrap.bundle.min.js")
	
	script.
		let allProducts = [];
		let allCategories = [];
		
		// Fetch all products from inventory
		async function loadProducts() {
			try {
				const response = await fetch('/inventory/api/all');
				if (!response.ok) throw new Error('Failed to fetch products');
				
				allProducts = await response.json();
				
				if (allProducts.length === 0) {
					document.getElementById('productsContainer').innerHTML = '<p class="text-center">No products available</p>';
					return;
				}
				
				// Get unique categories
				const categories = new Set(allProducts.map(p => p.category).filter(Boolean));
				allCategories = ['All', ...Array.from(categories)];
				
				// Generate category tabs
				generateCategoryTabs(categories);
				
				// Display products
				displayProducts(allProducts);
				
				// Setup event listeners
				setupEventListeners();
			} catch (error) {
				console.error('Error loading products:', error);
				document.getElementById('productsContainer').innerHTML = '<p class="text-center text-danger">Error loading products</p>';
			}
		}
		
		function generateCategoryTabs(categories) {
			const tabsList = document.getElementById('categoryTabs');
			// Clear existing tabs except "All"
			const allTab = tabsList.querySelector('#all');
			tabsList.innerHTML = '';
			tabsList.appendChild(allTab);
			
			// Add category tabs
			categories.forEach(category => {
				const li = document.createElement('li');
				const categoryId = category.toLowerCase().replace(/\s+/g, '-');
				li.id = categoryId;
				li.innerHTML = `
					<a href="javascript:void(0);">
						<img src="assets/img/products/pos-product-01.png" alt="Categories">
					</a>
					<h6>
						<a href="javascript:void(0);">${category}</a>
					</h6>
				`;
				
				li.addEventListener('click', function() {
					document.querySelectorAll('#categoryTabs li').forEach(t => t.classList.remove('active'));
					this.classList.add('active');
					const tabContent = document.querySelector(\`.tab_content[data-tab="\${categoryId}"]\`);
					if (tabContent) {
						document.querySelectorAll('.tab_content').forEach(t => t.classList.remove('active'));
						tabContent.classList.add('active');
					}
				});
				
				tabsList.appendChild(li);
			});
		}
		
		function displayProducts(products) {
			const container = document.getElementById('productsContainer');
			container.innerHTML = '';
			
			// Create "All" tab
			const allTab = document.createElement('div');
			allTab.className = 'tab_content active';
			allTab.setAttribute('data-tab', 'all');
			allTab.innerHTML = '<div class="row g-3" id="allProducts"></div>';
			container.appendChild(allTab);
			
			// Create tabs for each category
			const categories = new Set(products.map(p => p.category).filter(Boolean));
			categories.forEach(category => {
				const tab = document.createElement('div');
				const categoryId = category.toLowerCase().replace(/\s+/g, '-');
				tab.className = 'tab_content';
				tab.setAttribute('data-tab', categoryId);
				tab.innerHTML = \`<div class="row g-3" id="products-\${categoryId}"></div>\`;
				container.appendChild(tab);
			});
			
			// Populate products
			const allProductsDiv = document.getElementById('allProducts');
			const allProductsHTML = products.map(p => createProductHTML(p)).join('');
			allProductsDiv.innerHTML = allProductsHTML || '<p class="text-center">No products</p>';
			
			// Populate category products
			categories.forEach(category => {
				const categoryId = category.toLowerCase().replace(/\s+/g, '-');
				const categoryProducts = products.filter(p => p.category === category);
				const categoryDiv = document.getElementById(\`products-\${categoryId}\`);
				const categoryHTML = categoryProducts.map(p => createProductHTML(p)).join('');
				categoryDiv.innerHTML = categoryHTML || '<p class="text-center">No products in this category</p>';
			});
		}
		
		function createProductHTML(product) {
			return \`
				<div class="col-sm-6 col-md-6 col-lg-6 col-xl-4 col-xxl-3">
					<div class="product-info card mb-0">
						<a class="pro-img" href="javascript:void(0);">
							<img src="assets/img/products/pos-product-01.png" alt="Products">
							<span>
								<i class="ti ti-circle-check-filled"></i>
							</span>
						</a>
						<h6 class="cat-name">
							<a href="javascript:void(0);">${product.category || 'N/A'}</a>
						</h6>
						<h6 class="product-name">
							<a href="javascript:void(0);">${product.productName}</a>
						</h6>
						<div class="d-flex align-items-center justify-content-between price">
							<p class="text-gray-9 mb-0">₹\${parseFloat(product.sellingPrice).toFixed(2)}</p>
							<div class="qty-item m-0">
								<a class="dec d-flex justify-content-center align-items-center" href="javascript:void(0);" data-bs-toggle="tooltip" data-bs-placement="top" title="minus">
									<i class="ti ti-minus"></i>
								</a>
								<input class="form-control text-center" type="text" name="qty" value="1" data-product-id="\${product.id}">
								<a class="inc d-flex justify-content-center align-items-center" href="javascript:void(0);" data-bs-toggle="tooltip" data-bs-placement="top" title="plus">
									<i class="ti ti-plus"></i>
								</a>
							</div>
						</div>
					</div>
				</div>
			\`;
		}
		
		function setupEventListeners() {
			// Search functionality
			const searchInput = document.getElementById('searchProduct');
			searchInput.addEventListener('keyup', function() {
				const searchTerm = this.value.toLowerCase();
				const filtered = allProducts.filter(p => 
					p.productName.toLowerCase().includes(searchTerm) ||
					p.productCode?.toLowerCase().includes(searchTerm) ||
					p.category?.toLowerCase().includes(searchTerm)
				);
				displayProducts(filtered);
			});
			
			// Category tab switching
			document.querySelectorAll('#categoryTabs li').forEach(tab => {
				tab.addEventListener('click', function(e) {
					if (e.target.closest('a')) {
						e.preventDefault();
					}
					document.querySelectorAll('#categoryTabs li').forEach(t => t.classList.remove('active'));
					this.classList.add('active');
					
					const dataTab = this.id;
					document.querySelectorAll('.tab_content').forEach(t => t.classList.remove('active'));
					const activeTab = document.querySelector(\`.tab_content[data-tab="\${dataTab}"]\`);
					if (activeTab) {
						activeTab.classList.add('active');
					}
				});
			});
		}
		
		// Load products on page load
		document.addEventListener('DOMContentLoaded', loadProducts);
	
	script(src="/assets/js/pos.js")